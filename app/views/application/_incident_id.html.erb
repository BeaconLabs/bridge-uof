<% if value.present? %>
  <span class='ursus-id-component ursus-id-ursus' data-toggle='popover' data-trigger='hover' data-placement='top' data-content='Standard prefix for all incidents'><%= value.prefix %></span>-<span class='ursus-id-component ursus-id-county' data-toggle='popover' data-trigger='hover' data-placement='top' data-content='County code of agency'><%= value.county %></span>-<span class='ursus-id-component ursus-id-agency' data-toggle='popover' data-trigger='hover' data-placement='top' data-content='Identifier of agency (within the county)'><%= value.agency %></span>-<span class='ursus-id-component ursus-id-year' data-toggle='popover' data-trigger='hover' data-placement='top' data-content='Incident Year'><%= value.year %></span>-<span class='ursus-id-component ursus-id-code' data-toggle='popover' data-trigger='hover' data-placement='top' data-content='Random 3-letter identifier'><%= value.code %></span>
<% end %>
